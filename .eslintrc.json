{
  "root": true,
    "env": {
    "node": true,
    "browser": true,
    "es2022": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"

  },
  "ignorePatterns": [
    "node_modules/",
    "dist/",
    "build/",
    "frontend/dist/"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  }
  

}
